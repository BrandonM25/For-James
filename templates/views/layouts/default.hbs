</head>

<body>
	<!-- Main navigation -->
	<!--Navbar-->
	<nav class="navbar navbar-expand-lg navbar-dark fixed-top scrolling-navbar">
		<div class="container">
			<a class="navbar-brand" href="/">
				<strong>iJnApparel</strong>
			</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent-7" aria-controls="navbarSupportedContent-7"
			 aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarSupportedContent-7">
				<ul class="nav navbar-nav mr-auto navbar-left">
					{{# each navLinks}}
					<li {{#ifeq ../section key}}class="nav-item active" {{else}}{{/ifeq}}>
						<a class="nav-link" href="{{ href }}">{{ label }}</a>
					</li>
					{{/each}}
				</ul>
				<ul class="nav navbar-nav navbar-right">
					{{#if user}}
					<li><a class="nav-link" href="/keystone/signout">Sign Out</a></li>
					{{else}}
					<li><a class="nav-link" href="/keystone/signin">Sign In</a></li>
					{{/if}}
				</ul>
			</div>
		</div>
	</nav>
	<!-- Navbar -->

	<!-- The content block should contain the body of your template's content-->
	{{{body}}}

